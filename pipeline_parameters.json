{
  "dataset": {
    "center_mask_size": 25,
    "clipping_bounds": [
      -1000,
      400
    ],
    "context_window": 70,
    "data_augmentation": true,
    "dimensionality": "3D",
    "segment_nodule": "none"
  },
  "model": {
    "in_channels": 1,
    "num_classes": 5
  },
  "training": {
    "batch_size": 4,
    "cross_validation_folds": 5,
    "cv_train_folds": 5,
    "do_cross_validation": true,
    "dropout_rate": 0.3,
    "learning_rate": 0.001,
    "min_delta": 0.01,
    "num_epochs": 100,
    "num_workers": 4,
    "patience": 30
  }
}
