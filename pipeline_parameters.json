{
  "dataset": {
    "clipping_bounds": [-1000, 400],
    "context_window": 40,
    "data_augmentation": true,
    "dimensionality": "2.5D",
    "segment_nodule": "none"
  },
  "model": {
    "in_channels": 3,
    "num_classes": 5
  },
  "training": {
    "batch_size": 16,
    "cross_validation_folds": 5,
    "cv_train_folds": 5,
    "do_cross_validation": true,
    "dropout_rate": 0.3,
    "learning_rate": 0.001,
    "min_delta": 0.01,
    "num_epochs": 100,
    "num_workers": 8,
    "patience": 30
  }
}
